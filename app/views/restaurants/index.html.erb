<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% for restaurant in @restaurants %>
      <tr>
        <td><%= restaurant.name %></td>
        <td><%= restaurant.address %></td>
        <td><%= restaurant.city %></td>
        <td><%= restaurant.state %></td>
        <td><%= restaurant.zip %></td>
        <td><%= restaurant.phone %></td>
        <td><%= restaurant.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
